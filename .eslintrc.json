{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "linebreak-style": "off",
        // tab size 4
        "indent": ["error", 4]
    }
}
